{
  "name": "axios-curlirize",
  "version": "0.0.7",
  "description": "Axios third-party module to print all axios requests as curl commands in the console.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/babel  ./src --experimental --source-maps-inline -d ./dist && export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha --exit dist/test/*.test.js",
    "postinstall": "./node_modules/.bin/babel  ./src --experimental --source-maps-inline -d ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/augmnt/axios-curlirize.git"
  },
  "keywords": [
    "axios",
    "curl",
    "debug"
  ],
  "author": "Augmnt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/augmnt/axios-curlirize/issues"
  },
  "homepage": "https://github.com/augmnt/axios-curlirize#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "fancy-log": "^1.3.2"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "expect": "^23.1.0",
    "express": "^4.16.3",
    "mocha": "^5.2.0"
  }
}
